{
    "version": 2,
    "builds": [{
        "src": "til/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "build.sh",
        "use": "@vercel/static"
      }],
    
    "routes": [
        {
            "src": "/staticfiles/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "til/wsgi.py"
        }
    ]
}